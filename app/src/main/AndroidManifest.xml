<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.userinteraction">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UserInteraction">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BroadcastActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".NotifyActivity"
            android:exported="true">
        </activity>

        <receiver android:name=".ReceivedBroadCast" android:exported="true">
            <intent-filter>
                <action android:name="com.example.CUSTOM_INTENT">
                </action>
            </intent-filter>
        </receiver>

        <activity
            android:name=".SharedPrefActivity"
            android:exported="true">
        </activity>
        <activity
            android:name=".SMSReceived"
            android:exported="true">
        </activity>

    </application>
    <uses-permission android:name="android.permission.RECEIVE_SMS"></uses-permission>
    <uses-permission android:name="android.permission.READ_SMS"></uses-permission>
    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>

</manifest>